 return\r\n        }\r\n\r\n        //regra limpeza do 0\r\n        const clearDisplay = this.state.valorDisplay === '0' || this.state.clearDisplay\r\n\r\n        // mudança de estado\r\n        const atual = clearDisplay ? '' : this.state.valorDisplay\r\n        const valorDisplay = atual + n\r\n        this.setState({ valorDisplay, clearDisplay: false})\r\n\r\n        if (n !== '.') {\r\n            const i = this.state.atual\r\n            const novoValor = parseFloat(valorDisplay)\r\n            const valores = [...this.state.valores]\r\n            valores[i] = novoValor\r\n            this.setState ({valores})\r\n        }\r\n    }\r\n\r\n    render() {\r\n        \r\n        return (\r\n            <div className=\"Calculadora\">\r\n                <Display value={this.state.valorDisplay}/>\r\n                <Botao label=\"AC\" click={this.limparMemoria} triple/>\r\n                <Botao label=\"/\" click={this.setOperacao} operacao/>\r\n                <Botao label=\"7\" click={this.adicionarDigito}/>\r\n                <Botao label=\"8\" click={this.adicionarDigito}/>\r\n                <Botao label=\"9\" click={this.adicionarDigito}/>\r\n                <Botao label=\"*\" click={this.setOperacao} operacao/>\r\n                <Botao label=\"4\" click={this.adicionarDigito}/>\r\n                <Botao label=\"5\" click={this.adicionarDigito}/>\r\n                <Botao label=\"6\" click={this.adicionarDigito}/>\r\n                <Botao label=\"-\" click={this.setOperacao} operacao/>\r\n                <Botao label=\"1\" click={this.adicionarDigito}/>\r\n                <Botao label=\"2\" click={this.adicionarDigito}/>\r\n                <Botao label=\"3\" click={this.adicionarDigito}/>\r\n                <Botao label=\"+\"click={this.setOperacao} operacao/>\r\n                <Botao label=\"0\" click={this.adicionarDigito} double/>\r\n                <Botao label=\".\" click={this.adicionarDigito}/>\r\n                <Botao label=\"=\" click={this.setOperacao} operacao/>            \r\n                </div>\r\n        )\r\n    }\r\n}\r\n","C:\\Users\\Dale\\Desktop\\Programas\\Concluidos\\React\\calculadora\\src\\components\\botao\\Display.jsx",["31"],"import React from 'react'\r\n\r\nimport './Display.css'\r\n\r\nexport default props => <div className=\"Display\">{props.value}</div>","C:\\Users\\Dale\\Desktop\\Programas\\Concluidos\\React\\calculadora\\src\\components\\botao\\Botao.jsx",["32"],"import React from 'react'\r\n\r\nimport './Botao.css'\r\n\r\nexport default props => {\r\n    let classes = 'Botao '\r\n    classes += props.operacao ? 'operacao' : ''\r\n    classes += props.double ? 'double' : ''\r\n    classes += props.triple ? 'triple' : ''\r\n    return (\r\n    <button onClick= {e =>props.click &&  props.click(props.label)}\r\n     className={classes}> {props.label} </button>\r\n     ) \r\n}",{"ruleId":"33","severity":1,"message":"34","line":4,"column":8,"nodeType":"35","messageId":"36","endLine":4,"endColumn":11},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","severity":1,"message":"42","line":40,"column":26,"nodeType":"43","messageId":"44","endLine":40,"endColumn":30},{"ruleId":"45","severity":1,"message":"46","line":5,"column":1,"nodeType":"47","endLine":5,"endColumn":69},{"ruleId":"45","severity":1,"message":"46","line":5,"column":1,"nodeType":"47","endLine":14,"endColumn":2},"no-unused-vars","'App' is defined but never used.","Identifier","unusedVar","no-native-reassign",["48"],"no-negated-in-lhs",["49"],"no-eval","eval can be harmful.","CallExpression","unexpected","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]